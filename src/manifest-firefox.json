{
    "manifest_version": 3,
    "name": "Try WordPress",
    "description": "An extension that liberates your data into WordPress",
    "version": "0.0.1",
    "permissions": [
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "icons": {
        "32": "icons/icon-32.png",
        "128": "icons/icon-128.png"
    },
    "action": {
        "default_icon": {
            "32": "icons/icon-32.png",
            "128": "icons/icon-128.png"
        },
        "default_title": "Try WordPress"
    },
    "content_scripts": [
        {
            "js": [
                "content/index.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "background": {
        "scripts": [
            "background/index.js"
        ]
    },
    "sidebar_action": {
        "default_title": "Try WordPress",
        "default_icon": {
            "32": "icons/icon-32.png",
            "128": "icons/icon-128"
        },
        "default_panel": "sidebar/sidebar.html",
        "open_at_install": false
    }
}
